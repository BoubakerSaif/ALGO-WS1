ALGORITHM ticket_price
VAR
    ticket:INTEGER:=20;
    age:INTEGER;
BEGIN
IF (age<=10) THEN
    ticket:=4
ELSE_IF (age<=16) THEN
    ticket:=10
ELSE
    ticket:=20
END_IF
    
END

