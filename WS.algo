ALGORITHM Division_2
VAR
    count: INTEGER:=0
    num:INTEGER;   24/2 => 12/2 => 6/2 => 3/2 => 1 (rest 1)
BEGIN
WHILE (num%2=0) DO
    num:=num/2
    count:=count+1
END_WHILE
    Write(count)
END